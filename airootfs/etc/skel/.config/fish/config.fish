set fish_greeting

alias neofetch 'neofetch --source ~/.config/neofetch/icarus.ascii --colors 5 4 4 4 5 7 --ascii_colors 5 4'
alias msfconsole 'msfconsole -q'
alias clip 'xclip -selection clipboard'
alias cat 'bat --pager=never --theme="Dracula" --plain'
alias catn 'bat --pager=never --theme="Dracula" --number --decorations=always'
alias ls 'exa'
alias lst 'exa -T'
alias la 'exa -la'
alias lat 'exa -laT'
alias find 'fd'
alias finda 'fd -uu'
alias du 'dust'
alias grep 'rg'
alias grepb 'rg --binary'
alias top 'btm'
alias python 'bpython'
alias vim 'nvim'
alias ps 'procs --pager=disable'
alias pst 'procs --pager=disable --tree'

set -x EXA_COLORS "di=1;36:flag=4;32"

